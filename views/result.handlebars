<div class="container">
    <div class="jumbotron">
        <img src="/assets/images/friends.jpg" alt="friends" class="img-fluid img-thumbnail float-right">
        <h1 class="display-4" id="resultmessage"> Congradulation....... </h1>
        <h1 class="display-4"> we appreciate your time on our survey </h1>
        <p class="lead" id="friendfound">Wow... you got a grea friends... </p>
        <hr class="my-4">
        <a class="btn btn-primary btn-lg" href="/" role="button">home</a>
    </div>
</div>

<div class="container">
    <span id="resultcount">{{rowsLength}}</span>
    <div class="row">

        <div class="col-sm-1 col-md-4 col-4" id="100">
            <div class="progress">
                <div class="progress-bar bg-success" role="progressbar" style="width: 100%" aria-valuenow="100"
                    aria-valuemin="0" aria-valuemax="100"></div>
            </div>
            <br>
            <div class="card" style="width: 22rem;">
                <img class="card-img-top" src="/assets/images/{{this.match100.img_url}}" alt="  Image Isn't available">
                <div class="card-body">
                    <p class="card-text"> Congradulation.... You Matched 100% with {{this.match100.name}}</p>
                </div>
            </div>

        </div>


        <div class="col-sm-1 col-md-4" id="75">
            <div class="progress">
                <div class="progress-bar bg-warning" role="progressbar" style="width: 75%" aria-valuenow="75"
                    aria-valuemin="0" aria-valuemax="100"></div>
            </div>
            <br>
            <div class="card" style="width: 22rem;">
                <img class="card-img-top" src="/assets/images/{{this.match75.img_url}}" alt="  Image Isn't available">
                <div class="card-body">
                    <p class="card-text"> Congradulation.... You Matched 75% with {{this.match75.name}}</p>
                </div>
            </div>
        </div>

        <div class="col-sm-1 col-md-4" id="50">
            <div class="progress">
                <div class="progress-bar bg-danger" role="progressbar" style="width: 50%" aria-valuenow="50"
                    aria-valuemin="0" aria-valuemax="100"></div>
            </div>
            <br>
            <div class="card" style="width: 22rem;">
                <img class="card-img-top" src="/assets/images/{{this.match50.img_url}}" alt="   Image Isn't available">
                <div class="card-body">
                    <p class="card-text"> Congradulation.... You Matched 50% with {{this.match50.name}}</p>
                </div>
            </div>
        </div>

    </div>
</div>